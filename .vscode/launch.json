{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/src/cockpit.js",
			"stopOnEntry": false,
			"args": [],
			"cwd": "${workspaceRoot}",
			"runtimeExecutable": null,
			"runtimeArgs": [
				"--nolazy"
			],
			"env": {
				"NODE_ENV": "development",
				"DEV_MODE": "true",
				"USE_MOCK": "true",
				"HARDWARE_MOCK": "true",
				"GEO_MOCK": "true",
				"port": 8080,
				"DEBUG": "hardware, physical-interface",
				"configfile": "/tmp/rovconfig.json",
				"plugins__ui-manager__selectedUI": "new-ui",
				"pluginsDownloadDirectory": "/tmp/plugins"
			},
			"externalConsole": false,
			"sourceMaps": false,
			"outDir": null
		},
		{
			"name": "Attach",
            "type": "node",
            "request": "attach",
            "port": 5858,
            "address": "localhost",
            "restart": false,
            "sourceMaps": false,
            "outDir": null,
            "localRoot": "${workspaceRoot}",
            "remoteRoot": null
        
		}
	]
}